@page
@using My.Shared
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model MyFeatures.Pages.EmployeesPageModel
<div class="row">
     <h1 class="display-2">Employees</h1>
</div>
<div class="row">
    @foreach (Employee e in Model.Employees) {
        <div class="col-sm-3">
            <partial name="_Employee" model="e" />
        </div>
    }
</div>
